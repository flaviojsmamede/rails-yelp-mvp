<h1>Create new Restaurant</h1>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.select :category, collection: Restaurant::CATEGORIES  %>
  <%= f.submit %>
<% end %>

<%= link_to "See all restaurants", restaurants_path %>
